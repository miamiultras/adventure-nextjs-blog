// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Place {
  slug        String @id @default(cuid())
  title       String
  description String
  image       String
}

model Post {
  slug    String @id @default(cuid())
  title   String
  image   String
  excerpt String
}
